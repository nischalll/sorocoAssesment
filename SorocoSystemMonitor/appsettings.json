{
    "MonitoringSettings": {
        "IntervalSeconds": 5,
        "EnableConsoleOutput": true,
        "EnableFileLogging": true,
        "LogFilePath": "system_monitor.log"
    },
    "ApiSettings": {
        "Endpoint": "https://httpbin.org/post",
        "TimeoutSeconds": 30,
        "EnableApiIntegration": true
    },
    "Plugins": {
        "FileLogger": {
            "Enabled": true,
            "LogLevel": "Information"
        },
        "ApiPoster": {
            "Enabled": true,
            "RetryAttempts": 3,
            "RetryDelaySeconds": 5
        }
    }
}